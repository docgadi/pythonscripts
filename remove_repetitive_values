def solution(*args,num):
    args = list(args)
    nums_to_remove = []
    if (len(args) > 99):
        print("Only a maximum of 99 arguments can be supplied in this function. You supplied",len(args), "arguments")
        return
    for x in args:
        if  (args.count(x) > int(num)):
            nums_to_remove.append(x)
    nums_to_remove = list(dict.fromkeys(nums_to_remove))
    output = set(args) - set(nums_to_remove)
    print (output)
